{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "<IMAGE_REPO_NAME>",
      "image":
        "<AWS_ACCOUNT_ID>.dkr.ecr.<AWS_DEFAULT_REGION>.amazonaws.com/<IMAGE_REPO_NAME>:<IMAGE_TAG>",
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "essential": true,
      "memory": 128
    }
  ]
}
